{
    "name": "Long Weekend",
    "version": "1.0.0",
    "description": "Your Holiday Bot",
    "manifest_version": 3,
    "author": "Sampurna Chapagain",
    "action": {
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
    },
    "permissions": [
        "notifications",
        "alarms",
        "storage"
    ],
    "background": {
    	"service_worker": "background.js"
 },
 "icons": {
    "16": "weekend.png",
    "32": "weekend.png",
    "48": "weekend.png",
    "128": "weekend.png"
  }
}